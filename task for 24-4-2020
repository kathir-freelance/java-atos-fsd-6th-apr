List<String> a=new ArrayList();
		a.add("hi");
		a.add("hello");
		a.add("welcome");
		a.add("When");
		a.add("help");
		a.add("hire");
		a.add("india");
		a.add("race");
		a.add("in");
		
		Stream<String> s1=a.stream();
		List l1=s1.map(x->x.length()).filter(x->x>3).collect(Collectors.toList());
		System.out.println(l1);

